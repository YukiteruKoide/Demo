   "source": [
    "SELECT\n",
    "  c.customer_id,\n",
    "  cs.total_spent,\n",
    "  cs.transaction_count\n",
    "FROM\n",
    "  users.yukiteru_koide.customer_summary_table cs\n",
    "JOIN\n",
    "  users.yukiteru_koide.customers_table c\n",
    "  ON cs.customer_id = c.customer_id\n",
    "ORDER BY\n",
    "  cs.total_spent DESC\n",
    "LIMIT 10;\n"
   ]
  }
