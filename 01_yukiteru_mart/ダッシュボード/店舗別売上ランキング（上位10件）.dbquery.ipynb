   "source": [
    "SELECT\n",
    "  s.store_name,\n",
    "  SUM(ss.total_sales) AS total_sales\n",
    "FROM\n",
    "  users.yukiteru_koide.store_sales_summary_table ss\n",
    "JOIN\n",
    "  users.yukiteru_koide.stores_table s\n",
    "  ON ss.store_id = s.store_id\n",
    "GROUP BY\n",
    "  s.store_name\n",
    "ORDER BY\n",
    "  total_sales DESC\n",
    "LIMIT 10;\n"
   ]
  }
